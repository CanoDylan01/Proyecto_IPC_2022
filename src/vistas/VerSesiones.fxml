<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" stylesheets="@../estilos/login.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.VerSesionesController">
   <children>
      <HBox prefHeight="35.0" prefWidth="600.0" VBox.vgrow="NEVER">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Inicio">
                     <HBox.margin>
                        <Insets left="20.0" right="10.0" />
                     </HBox.margin>
                  </Label>
                  <DatePicker />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Fin">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Label>
                  <DatePicker>
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </DatePicker>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="tableView_Sesiones" prefHeight="223.0" prefWidth="600.0" styleClass="table-cell" HBox.hgrow="ALWAYS">
              <columns>
                  <TableColumn fx:id="colFecha" minWidth="0.0" prefWidth="93.60003662109375" text="Fecha" />
                <TableColumn fx:id="colAcertadas" prefWidth="87.20001220703125" text="Acertadas" />
                  <TableColumn fx:id="colFalladas" minWidth="0.0" prefWidth="93.60003662109375" text="Falladas" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
      <HBox fillHeight="false" prefHeight="60.0" prefWidth="600.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="btn_volver" mnemonicParsing="false" onMouseClicked="#click_volver" text="Volver">
               <font>
                  <Font size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
